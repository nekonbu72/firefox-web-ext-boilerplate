{
  "description": "description of the extension",
  "manifest_version": 2,
  "name": "Myextension",
  "version": "1.0",
  "icons": {
    "48": "assets/icons/beasts-48.png"
  },
  "permissions": ["<all_urls>", "activeTab", "tabs"],
  "browser_action": {
    "default_icon": "assets/icons/beasts-32.png",
    "default_title": "Myextension"
  },
  "web_accessible_resources": ["popup/index.html"],
  "background": {
    "scripts": ["background_scripts/index.js"]
  }
}
